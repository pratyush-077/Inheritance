#include <iostream>
using namespace std;

class A {
public:
    void showA() { cout << "Class A" << endl; }
};

class B : public A {
public:
    void showB() { cout << "Class B" << endl; }
};

class C {
public:
    void showC() { cout << "Class C" << endl; }
};

class D : public B, public C {  // Hybrid: from B (which comes from A) + C
public:
    void showD() { cout << "Class D" << endl; }
};

int main() {
    D obj;
    obj.showA();
    obj.showB();
    obj.showC();
    obj.showD();
    return 0;
}

//OUTPUT:
//Class A
//Class B
//Class C
//Class D
